{
    "name": "osddqd/http-client-generator-for-laravel",
    "description": "Enhanced HTTP Client Generator for Laravel with custom namespace and path support",
    "license": "MIT",
    "authors": [
        {
            "name": "OSDDQD",
            "email": "osddqd@users.noreply.github.com"
        },
        {
            "name": "Janez Cergolj",
            "email": "me@jcergolj.me.uk"
        }
    ],
    "keywords": ["laravel", "http-client", "generator", "artisan", "command"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/prompts": "^0.3.1"
    },
    "require-dev": {
        "laravel/pint": "^1.9",
        "tightenco/tlint": "^9.1",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Osddqd\\HttpClientGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osddqd\\HttpClientGenerator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "tlint": "tlint",
        "analyse": [
            "@pint",
            "@tlint"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osddqd\\HttpClientGenerator\\HttpClientGeneratorServiceProvider"
            ]
        }
    }
}
